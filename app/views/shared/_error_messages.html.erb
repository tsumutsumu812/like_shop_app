<% if @user.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
    <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
    <%#>  The form contains <%= pluralize(@user.errors.count, "error") %>
    </div>
    
  </div>
<% end %>
